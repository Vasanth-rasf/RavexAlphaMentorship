{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/submit-application",
      "dest": "/api/submit-application.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}

