{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/submit-application.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/submit-application",
      "dest": "/api/submit-application.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}

